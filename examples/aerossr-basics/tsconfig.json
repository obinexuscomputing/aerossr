{
  "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "declaration": true,
      "sourceMap": true,
      "allowJs": true
    
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}